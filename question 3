def find_largest_number(numbers):
    largest = numbers[0]
    for num in numbers:
        if num > largest:
            largest = num
    return largest
input_numbers = list(map(int, input('1,2,3,4,5,6,7,9,8').split()))
largest_number = find_largest_number(input_numbers)
print("The largest number is:", largest_number)
